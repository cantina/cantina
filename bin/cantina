#!/usr/bin/env node

var cantina = require('../'),
    path = require('path');

// Set up the cli app.
var app = cantina.createApp({
  mode: 'cli',
  dir: path.resolve(__dirname, '..', 'lib/cli'),
  usage: [
    '',
    'Usage: cantina <command>',
    '',
    'where <command> is one of:',
    '    create',
    ''
  ]
});

app.start();